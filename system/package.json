  "bugs": {
    "url": "https://github.com/fmlimao/cast/issues"
  },
  "homepage": "https://github.com/fmlimao/cast#readme",
  "dependencies": {
    "axios": "^0.20.0",
    "cookie-parser": "^1.4.5",
    "dotenv-safe": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "helmet": "^4.1.0",
    "morgan": "^1.10.0",
    "startbootstrap-sb-admin-2": "^4.1.1",
    "vue": "^2.6.12"
  }
}
